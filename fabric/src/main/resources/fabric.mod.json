{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "Jozufozu",
    "PepperCode1"
  ],
  "contact": {
    "homepage": "${mod_homepage}",
    "sources": "${mod_sources}",
    "issues": "${mod_issues}"
  },
  "license": "${mod_license}",
  "icon": "logo.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "dev.engine_room.flywheel.impl.FlywheelFabric"
    ]
  },
  "mixins": [
    "flywheel.backend.mixins.json",
    "flywheel.impl.mixins.json",
    "flywheel.impl.fabric.mixins.json"
  ],
  "depends": {
    "minecraft": "${minecraft_semver_version_range}",
    "fabric-api": "${fabric_api_version_range}"
  },
  "breaks": {
    "sodium": ["<0.6.0-beta.2"]
  }
}
